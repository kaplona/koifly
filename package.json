{
  "name": "koifly-flight-loging-app",
  "version": "0.2.0",
  "description": "Flight loging app for hang glider and paraglider pilots",
  "keywords": [
    "hapi",
    "react",
    "webpack",
    "HMR"
  ],
  "license": "MIT",
  "author": "Anastasia Kaplina <nkaplina@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaplona/koifly-flight-loging-app"
  },
  "main": "src/server.js",
  "scripts": {
    "forever_start": "NODE_ENV=production forever start src/server.js",
    "start": "NODE_ENV=production     node src/server.js",
    "build": "NODE_ENV=production     node src/scripts/make.js  &&  NODE_ENV=production   webpack --config src/config/webpack-config.js",
    "webpack": "NODE_ENV=development  node src/scripts/make.js  &&  NODE_ENV=development  node src/webpack-dev-server.js",
    "dev": "NODE_ENV=development      node src/scripts/make.js  &&  NODE_ENV=development  nodemon src/server.js",
    "make": "NODE_ENV=development     node src/scripts/make.js",
    "db_sync": "MYSQL_ADMIN=true      node src/orm/sync.js",
    "db_migrate": "MYSQL_ADMIN=true      ./node_modules/.bin/sequelize db:migrate",
    "lint": "eslint . --ext .js --ext .jsx",
    "lint_fix": "eslint . --ext .js --ext .jsx --fix",
    "test": "NODE_ENV=testing ./node_modules/.bin/mocha --config .mocharc.json --recursive ./tests/"
  },
  "dependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-transform-classes": "7.4.4",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "@hapi/cookie": "10.1.0",
    "@hapi/hapi": "18.4.1",
    "@hapi/inert": "5.2.0",
    "@hapi/vision": "5.5.2",
    "bcrypt": "3.0.6",
    "chalk": "2.4.2",
    "csvtojson": "1.1.9",
    "eventemitter3": "4.0.0",
    "google-maps-api": "2.0.1",
    "hapi-react-views": "10.0.1",
    "highcharts": "7.0.2",
    "history": "4.9.0",
    "less": "3.9.0",
    "lodash.escape": "4.0.1",
    "lodash.isequal": "4.5.0",
    "lodash.orderby": "4.6.0",
    "lodash.sample": "4.2.1",
    "lodash.shuffle": "4.2.0",
    "mysql2": "1.6.5",
    "nodemailer": "6.2.1",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0",
    "rimraf": "2.6.1",
    "sequelize": "5.21.4",
    "sequelize-cli": "5.5.0"
  },
  "devDependencies": {
    "@hapi/h2o2": "8.3.0",
    "@testing-library/react": "9.4.0",
    "assets-webpack-plugin": "3.9.10",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "chai": "4.2.0",
    "css-loader": "3.0.0",
    "eslint": "6.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-react": "7.14.2",
    "express": "4.17.1",
    "ignore-styles": "5.0.1",
    "jsdom": "16.2.0",
    "jsdom-global": "3.0.2",
    "less-loader": "5.0.0",
    "mini-css-extract-plugin": "0.7.0",
    "mocha": "7.0.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "react-hot-loader": "4.12.8",
    "sinon": "8.1.1",
    "sinon-chai": "3.4.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "2.3.5",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.7",
    "webpack-dev-middleware": "3.7.0",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "4.2.1"
  },
  "peerDependencies": {
    "forever": "0.x"
  }
}
